package main

type getPortfolioValueResp struct {
	Dates  []string `json:"dates"`
	Values []string `json:"values"`
}

func getPortfolioValue() {
	request, err := http.NewRequest("GET", "http://portfolio-service/get-portfolio-value", nil)
	if err != nil {
		app.errorJSON(w, err)
		return
	}

	client := &http.Client{}
	response, err := client.Do(request)
	if err != nil {
		app.errorJSON(w, err)
		return
	}
	defer response.Body.Close()

